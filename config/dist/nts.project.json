{
  "spot-local-dev": {
    "name": "spot-local-dev",
    "groups": [
      "nts"
    ],
    "default_version": "master",
    "build_cmd": [
      "docker build -t poloniex-local/flyway ./db/."
    ]
  },
  "spot-common": {
    "name": "spot-common",
    "groups": [
      "nts"
    ],
    "default_version": "BRANCH_PHASE2",
    "depends_on": {
      "compile": [
        "spot-proto",
        "spot-local-dev"
      ]
    }
  },
  "spot-proto": {
    "name": "spot-proto",
    "groups": [
      "nts"
    ],
    "default_version": "BRANCH_PHASE2",
