        "spot-local-dev"
      ]
    }
  },
  "spot-account": {
    "name": "spot-account",
    "groups": [
      "nts"
    ],
    "default_version": "master",
    "depends_on": {
      "compile": [
        "spot-local-dev",
        "spot-common",
        "spot-proto"
      ]
    },
    "flyway_path": "db/general/spot_account"
  },
